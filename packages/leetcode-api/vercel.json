{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "schedule": "*/1 * * * *",
            "path": "./src/scripts/updateDaily.js"
        }
    ]
}